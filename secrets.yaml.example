# Example secrets file - Copy this to secrets.yaml and fill in your values

# WiFi credentials
wifi_ssid: "Your_WiFi_SSID"
wifi_password: "your_wifi_password"

# Home Assistant API
ha_api_key: "your_home_assistant_api_key"

# OTA password (for over-the-air updates)
ota_password: "your_ota_password"

# Fallback hotspot password
ap_password: "fallback_password"
